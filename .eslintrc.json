{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "indent"           : [1, 4],
        "linebreak-style": 0,
        "no-underscore-dangle": "off",
        "import/extensions": { "js": "always"}
    }
}
